<h1>Text adventures you can play</h1>
<div class="project-container">
<% textAdv.forEach( adv=> { %>
    <div>
        <a href="/TextAdventure/<%= `${adv.id}-i-i` %>" class='btn-link'>
            <button class="title-btn">
                <%= adv.title %>
            </button>
        </a>
    </div>
<% }) %>
</div>